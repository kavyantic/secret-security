                                    <thead>
                                        <tr>
                                            <th>Username</th>
                                            <th>Password</th>
                                            <th>Email</th>
                                            <th>Balance</th>
                                            <th>Phone</th>
                                            <th>Retailers</th>
                                            <th>Edit</th>

                                        </tr>
