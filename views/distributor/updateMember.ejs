<%-include('partials/header') %>
                    
                    <!-- Page Heading -->
                    <div class="container mt-5">
                        <div class="row">
                          
                          <div class="col-sm-9  ">
                            <div class="card">
                              <div class="card-body">
                                <h3 class="login-page-heading">Update Member</h3>
                      
                                <!-- Makes POST request to /login route -->
                                  
                                <form actionid="signup-form" action="/admin/members/update/<%=info.userType%>/<%=info.userName%>" method="post" id="admin-register-member">
                                  <div class="row">
                                    <div class="col">
                                      <div class="form-group input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"> <i class="fa fa-user"></i> </span>
                                         </div>
                                            <input name="username" class="form-control" placeholder="Username" value="<%=info.auth.username %>"  type="text" >
                                        </div> <!-- form-group// -->
                                        <div class="form-group input-group">
                                          <div class="input-group-prepend">
                                            <span class="input-group-text"> <i class="fa fa-envelope"></i> </span>
                                         </div>
                                            <input name="email" class="form-control" value=<%=info.auth.email %> placeholder="Email address" type="email">
                                        </div> 
                                        <!-- <div class="form-group input-group" id="sponsered-by-input">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"> <i class="fas fa-asterisk"></i> </span>
                                            </div>
                                            <input name="sponser" class="form-control" placeholder="Sponser ID">
                                        </div>  -->
                                        
                                        <!-- form-group// -->
                                        <div class="form-group input-group">
                                          <div class="input-group-prepend">
                                            <span class="input-group-text"> <i class="fa fa-phone"></i> </span>
                                        </div>
                                        <select  name="countryCode" class="custom-select" style="max-width: 100px;">
                                            <option selected="">+91</option>
                                            <!-- <option value="1">+972</option>
                                            <option value="2">+198</option>
                                            <option value="3">+701</option> -->
                                        </select>
                                          <input name="phone" class="form-control" value=<%=info.auth.phone%> placeholder="Phone number" type="text">
                                        </div> 
                                       <!-- form-group end.// -->
                                        <div class="form-group input-group">
                                          <div class="input-group-prepend">
                                            <span class="input-group-text"> <i class="fa fa-lock"></i> </span>
                                        </div>
                                            <input name="password" class="form-control" value=<%=info.auth.password %> placeholder="Create password" type="text" >
                                        </div> 
                                      <div class="custom-control custom-checkbox">
                                        <input name="wangchuk" value="go" type="checkbox" class="custom-control-input" id="customCheck1">
                                        <label class="custom-control-label" for="customCheck1">Remeber me</label>
                                          </div>
                                      </div>                                    
                                    <div class="col">
                                            <div hide="retailer" class="form-check">
                                              <input name="canRegisterAccount " class="form-check-input" type="checkbox" value="true" id="flexCheckChecked" <%=info.auth.canRegisterMember && "checked"%>>
                                              <label class="form-check-label" for="flexCheckChecked">
                                                Can register member
                                              </label>
                                            </div>
                                            <hr>
                                            <div class="form-check">
                                              <input name="canSetServiceTime" class="form-check-input" type="checkbox" value="false" id="canSetServiceTime" <%=info.auth.canSetServiceTime && "checked"%>>
                                              <label class="form-check-label" for="canSetServiceTime">
                                                Can set service time
                                              </label>
                                            </div>
                                            <hr>
                                            <div class="form-check">
                                              <input name="canAddMoney" class="form-check-input" type="checkbox" value="true" id="canAddMoney" <%=info.auth.canAddMoney && "checked"%>>
                                              <label class="form-check-label" for="flexCheckChecked">
                                                 Can add money
                                              </label>
                                            </div>
                                            <hr>
                                            <div class="form-check">
                                              <input name= "canDeductMoney" class="form-check-input" type="checkbox" value="false" id="canDeductMoney" <%=info.auth.canDeductMoney && "checked"%>>
                                              <label class="form-check-label" for="flexCheckChecked">
                                                Can deduct money
                                              </label>
                                            </div>
                                            <hr>
                                            <div class="form-check">
                                              <input name="canUploadBills" class="form-check-input disabled" type="checkbox" value="true" id="canUploadBills" <%=info.auth.canUploadBills && "checked"%>>
                                              <label class="form-check-label" for="flexCheckChecked">
                                                Can upload bills
                                              </label>
                                            </div>
                                            <hr>
                                            <div class="form-check">
                                              <input name="canViewReport" class="form-check-input" type="checkbox" value="true" id="flexCheckChecked" <%=info.auth.canViewReport && "checked"%>>
                                              <label class="form-check-label" for="flexCheckChecked">
                                                Can view reports
                                              </label>
                                          </div>
                                      </div>
                                </div>                                    
                           

                                  
                                 
                                 
                                    <!-- form-group// -->  
                                    <h3 class="err-msg" style="color:red;font-size:1em;padding:4%;"><%=info.msg%></h3>                                                         
                                                          
                                    <div class="form-group">
                                        <button type="submit" class="btn btn-primary btn-block"> Save </button>
                                    </div>
                                     <!-- form-group// -->      
                                </form>
                              </div>
                            </div>
                          </div>
                      
                          <!-- <div class="col-sm-4">
                            <div class="card">
                              <div class="card-body">
                                <a class="btn btn-block" href="/auth/google" role="button">
                                  <i class="fab fa-google"></i>
                                  Sign In with Google
                                </a>
                              </div>
                            </div>
                          </div> -->
                      
                        </div>
                      </div>

<%-include('partials/footer',{path:'register'}) %>
